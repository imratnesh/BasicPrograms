<beans xmlns="http://www.springframework.org/schema/beans"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns:p="http://www.springframework.org/schema/p"
xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
    
<!--<bean id="helloWorld" class="com.tutorial.HelloWorld" init-method="init" destroy-method="destroy">-->
<bean id="javaCollection" class="com.Values.tutorials.JavaCollection" >
    <!--<constructor-arg ref="spellChecker"></constructor-arg>-->
    <property name="countryList">
        <list>
            <value>USA</value>
            <value>UK</value>
            <value>India</value>
            <value>China</value>
            <value>UK</value>
        </list>
    </property>
    <property name="countrySet">
        <list>
            <value>USA</value>
            <value>UK</value>
            <value>India</value>
            <value>China</value>
            <value>UK</value>
        </list>
    </property>
    <property name="countryMap">
        <map>
<entry key="1" value="NDIA"/>
<entry key="2" value="Pakistan"/>
<entry key="3" value="USA"/>
<entry key="4" value="USA"/>
</map>
    </property>
   
    <property name="countryProperties"> 
    <props>
<prop key="one">INDIA</prop>
<prop key="two">Pakistan</prop>
<prop key="three">USA</prop>
<prop key="four">USA</prop>
</props>
    </property>
</bean>


<!--<bean class="com.tutorial.InitHelloWorld" />-->
</beans>